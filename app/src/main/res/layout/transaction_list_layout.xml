<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <LinearLayout
        android:id="@+id/tra_tablayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">



            <TableLayout
                android:id="@+id/tra_tablayout1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TableRow>

                    <LinearLayout
                        android:layout_width="@dimen/_56sdp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_weight="0"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/date_transaction_all_list1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingTop="2sp"
                            android:text="20 Aug 2023"
                            android:textColor="?attr/listItemTextColor"
                            android:textSize="@dimen/_10sdp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="@dimen/_85sdp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_20sdp"
                        android:layout_weight="0"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/category_transaction_all_list1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Other Income"
                            android:textAlignment="center"
                            android:textColor="?attr/listItemTextColor"
                            android:textSize="@dimen/_10sdp" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="@dimen/_50sdp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_15sdp"
                        android:layout_weight="0"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/income_transaction_all_list1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="1000"
                            android:textAlignment="center"
                            android:textColor="#1D5A20"
                            android:textSize="@dimen/_10sdp" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="@dimen/_50sdp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_20sdp"
                        android:layout_weight="0"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/expense_transaction_all_list1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="1000"
                            android:textAlignment="center"
                            android:textColor="#E63131"
                            android:textSize="@dimen/_10sdp" />
                    </LinearLayout>

                </TableRow>

            </TableLayout>

            <TableLayout
                android:id="@+id/tra_tablayout2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TableRow>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/time_transaction_all_list1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="02:50 PM"
                            android:textColor="?attr/listItemTextColor"
                            android:textSize="@dimen/_10sdp" />
                    </LinearLayout>

                </TableRow>

            </TableLayout>

            <TableLayout
                android:id="@+id/tra_tablayout3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TableRow>

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingBottom="@dimen/_2sdp">

                        <TextView
                            android:id="@+id/paymentmethod_transaction_all_list1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:background="@drawable/edttxt_bg"
                            android:paddingStart="@dimen/_5sdp"
                            android:paddingEnd="@dimen/_5sdp"
                            android:text="Cash"
                            android:textAlignment="center"
                            android:textColor="?attr/listItemTextColor"
                            android:textSize="@dimen/_10sdp" />

                    </RelativeLayout>


                </TableRow>

            </TableLayout>

            <TableLayout
                android:id="@+id/tra_tablayout4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TableRow>

                    <TextView
                        android:id="@+id/note_transaction_all_list1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_weight="0"
                        android:paddingBottom="@dimen/_2sdp"
                        android:text="From : Cash"
                        android:textColor="?attr/listItemTextColor"
                        android:textSize="@dimen/_10sdp" />

                </TableRow>

            </TableLayout>

    </LinearLayout>


</LinearLayout>
